<IfModule mod_rewrite.c>
	RewriteEngine On
	
	RewriteCond %{REQUEST_URI} ^/d/([a-zA-Z0-9_]+)\/([a-zA-Z0-9_]+)(.*)?$ 
	RewriteRule .* http://dicom.local/index.php?c=%1&m=%2%3
	
	RewriteCond %{REQUEST_URI} ^/a/([a-zA-Z0-9_]+)\/([a-zA-Z0-9_]+)(.*)?$ 
	RewriteRule .* http://dicom.local/index.php?x=1&c=%1&m=%2%3
	
</IfModule>
